{
  "name": "hellobooks",
  "version": "0.1.0",
  "public": true,
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books. Edit Add topics",
  "scripts": {
    "start": "node ./dist/app",
    "dev": "nodemon ./dist/app",
    "test": "NODE_ENV=production sequelize db:migrate:undo:all && sequelize db:migrate && mocha --require babel-polyfill --compilers js:babel-register",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server --watch -s -d dist",
    "build:prod": "npm run build && node entry"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "~4.13.4",
    "express-session": "^1.15.5",
    "express-validator": "^3.2.1",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^7.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbrandydude/hellobooks.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbrandydude/hellobooks/issues"
  },
  "homepage": "https://github.com/dbrandydude/hellobooks#readme"
}
